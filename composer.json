{
  "name": "omnireceipt/dummy",
  "description": "Dummy driver for Omnireceipt fiscal receipt processing library",
  "version": "2.1.0",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "receipt",
    "fiscal",
    "omnireceipt",
    "dummy"
  ],
  "homepage": "https://github.com/omnireceipt/dummy",
  "authors": [
    {
      "name": "Alexander Arhitov",
      "email": "clgsru@gmail.com",
      "homepage": "https://github.com/arhitov/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Omnireceipt\\Dummy\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omnireceipt\\Dummy\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.0",
    "omnireceipt/common": "^1.4",
    "nesbot/carbon": "^2",
    "doctrine/collections": "^1.8"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^2.6",
    "phpunit/phpunit": "^9",
    "mockery/mockery": "^1.6",
    "php-http/curl-client": "^2.3",
    "php-http/mock-client": "^1.6"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
